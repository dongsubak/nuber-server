{
  "name": "nuber-server",
  "version": "1.0.0",
  "description": "Server for the Nuber Clone Course. GraphQL, Typescript, NodeJS",
  "main": "index.js",
  "repository": "https://github.com/dongsubak/nuber-server",
  "author": "dongsubak <likeap@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.7",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mailgun-js": "^0.22.11",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.2",
    "@types/twilio": "^2.11.0",
    "babel-runtime": "^6.26.0",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "predev": "yarn run types",
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts,graphql",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "global": "^4.4.0",
    "graphql-tools": "^6.2.2",
    "graphql-yoga": "^1.18.3",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^9.0.0",
    "mailgun-js": "^0.22.0",
    "merge-graphql-schemas": "^1.7.8",
    "morgan": "^1.10.0",
    "pg": "^8.4.2",
    "tslint": "^6.1.3",
    "twilio": "^3.54.1",
    "typeorm": "^0.2.29"
  }
}
